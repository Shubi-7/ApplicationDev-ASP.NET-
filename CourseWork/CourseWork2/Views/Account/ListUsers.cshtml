    @model IEnumerable<CourseWork2.Models.UpdateViewModel>
    @{

        Layout = "~/Views/Shared/_DashboardLayout.cshtml";
    }

    <h2>View User List</h2>

    <div class="container-fluid">
        <a class="btn btn-outline-primary" href="@Url.Action("Register", "Account")">Add New User</a>
    </div>

    <table class="table table-hover ">
        <tr>
            <th>
                UserName
            </th>
            <th>
                Email
            </th>
            <th>
                Phone Number
            </th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.UserName
                </td>
                <td>
                    @item.Email
                </td>
                <td>
                    @item.PhoneNumber
                </td>


            </tr>
        }
    </table>
